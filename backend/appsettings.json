{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LanguageTool": {
    "BaseUrl": "http://localhost:8010"
  },
  "UniversityConfig": {
    "Name": "Default University",
    "CheckGrammar": false,
    "Language": "pl-PL",
    "Formatting": {
      "Font": {
        "FontFamily": "Times New Roman",
        "FontSize": 12
      },
      "Layout": {
        "MarginLeft": 2.5,
        "MarginRight": 2.5,
        "RequiredIndentCm": 1.25,
        "ParagraphSpacingRule": [0, 6]
      }
    }
  }
}
